{
  "name": "our-wallet-log-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:sqlite:generate": "DATABASE_URL=\"file:./dev.db\" prisma generate --schema=prisma/sqlite/schema.prisma",
    "db:sqlite:migrate": "DATABASE_URL=\"file:./dev.db\" prisma migrate dev --schema=prisma/sqlite/schema.prisma",
    "dev:sqlite": "npm run db:sqlite:generate && DATABASE_URL=\"file:./dev.db\" next dev"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.8",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "jose": "^6.1.3",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^7.2.0",
    "typescript": "^5"
  }
}
